{
    "data": "data.csv",
    "mapping": {
        "x": "year",
        "y": "value",
        "g": "g2",
        "f": "g2"
    },
    "x": {
        "column": "year",
        "axis": {
            "ticks": 3,
            "label": "Year",
            "format": ""
        }
    },
    "y": {
        "column": "value",
        "domain": [0, null],
        "axis": {
            "ticks": 3,
            "label": "Value"
        }
    },
    "f": {

    },
    "g": {
        "common": {
            "opacity": 0.2,
            "stroke-width": 2
        },
        "manual": {
            "A": {
                "label": "alt",
                "color": "red"
            },
            "B": {
                "label": "mittel",
                "color": "blue",
                "stroke-width": 3
            },
            "C": {
                "label": "neu",
                "color": "green"
            },
            "D": {
                "label": "bla",
                "color": "yellow"
            }
        }
    },
    "paths": {
        "attrs": {
            "stroke-width": ":stroke-width",
            "fill": "none",
            "opacity": ":opacity",
            "stroke": ":color"
        }
    },
    "circles": {
        "attrs": {
            "opacity": ":opacity",
            "r": 5,
            "fill": ":color"
        }
    },
    "lines": {
        "attrs": {
            "opacity": 1,
            "stroke": ":color"
        }
    },
    "rects": {
        "attrs": {
            "fill": ":color"
        }
    }
}
