{
    "data": "./data/immo/ind-full-final-l.csv",
    "options": {
        "height": 400,
        "margins": {"top": 20, "right": 10, "bottom": 50, "left": 70},
        "title": "Mietpreis",
        "subtitle": "Preis in €/m²",
        "footer": "Q: Mikrozensus."
    },
    "mapping": {
        "x": {
            "column": "year",
            "type": "numeric",
            "scale": {
                "type": "linear",
                "range": "horizontal"
            },
            "axis": {
                "ticks": 6,
                "format": "c",
                "position": "bottom"
            }
        },
        "y": {
            "column": "value",
            "type": "numeric",
            "scale": {
                "type": "linear",
                "domain": [0, null],
                "range": "vertical",
                "format": ".2f"
            },
            "axis": {
                "ticks": 8,
                "position": "left",
                "grid": true,
                "format": ".1f"
            },
            "hover": {
                "format": ".2f"
            }
        },
        "quali": {
            "column": "quali",
            "legend": {
                "props": {
                    "class": "@class"
                },
                "symbol": {
                    "size": 16,
                    "elements": [{
                        "type": "line",
                        "props": {
                            "x1": 0, "x2": 16, "y1": 8, "y2": 8,
                            "stroke": "#888", "stroke-width": 2
                        }
                    }]
                }
            },
            "props": {
                "common": {
                    "visible": false
                },
                "manual": {
                    "A":  {  },
                    "B":  {  },
                    "C":  {  },
                    "D":  {  },
                    "NA": {  },
                    "Alle": { "visible": true, "class": "bold" }
                }
            },
            "hover": {}
        },
        "werr": {
            "column": "werr.label",
            "legend": {
                "symbol": {
                    "size": 16,
                    "elements": [{
                        "type": "line",
                        "props": {
                            "x1": 0, "x2": 16, "y1": 8, "y2": 8,
                            "stroke": "#888", "stroke-width": 2
                        }
                    }]
                }
            },
            "props": {
                "common": {
                    "visible": false
                },
                "manual": {
                    "1918 und davor": { },
                    "1919 - 1944":    { },
                    "1945 - 1960":    { },
                    "1961 - 1970":    { },
                    "1971 - 1980":    { },
                    "1981 - 1990":    { },
                    "1991 - 2000":    { },
                    "2001 - 2010":    { },
                    "2011 - 2020":    { },
                    "NA":             { },
                    "Alle": { "visible": true, "class": "bold" }
                }
            },
            "hover": {}
        },
        "wfrist": {
            "column": "wfrist.label",
            "legend": {
                "symbol": {
                    "size": 16,
                    "elements": [{
                        "type": "line",
                        "props": {
                            "x1": 0, "x2": 16, "y1": 8, "y2": 8,
                            "stroke": "#888", "stroke-width": 2
                        }
                    }]
                }
            },
            "props": {
                "common": {
                    "visible": false
                },
                "manual": {
                    "unbefristet": { },
                    "befristet": { },
                    "NA": {  },
                    "Alle": { "visible": true }
                }
            },
            "hover": {}
        },
        "wrecht": {
            "column": "wrecht.miete",
            "legend": {
                "symbol": {
                    "size": 16,
                    "elements": [{
                        "type": "line",
                        "props": {
                            "x1": 0, "x2": 16, "y1": 8, "y2": 8,
                            "stroke": "#888", "stroke-width": 2,
                            "stroke-dasharray": "@stroke-dasharray"
                        }
                    }]
                }
            },
            "props": {
                "common": {
                    "visible": false
                },
                "manual": {
                    "gbv": { "name": "GBV", "stroke-dasharray": null },
                    "pub": { "name": "öffentlich", "stroke-dasharray": 2 },
                    "priv": { "name": "privat", "stroke-dasharray": 4 },
                    "NA": { "stroke-dasharray": "2 4" },
                    "Alle": { "visible": true, "class": "bold" }
                }
            },
            "props": {
                "common": {
                    "visible": false
                },
                "manual": {
                    "gbv": { "name": "GBV", "stroke-dasharray": null },
                    "pub": { "name": "öffentlich", "stroke-dasharray": 2 },
                    "priv": { "name": "privat", "stroke-dasharray": 4 },
                    "NA": { "stroke-dasharray": "2 4" },
                    "Alle": { "visible": true, "class": "bold"  }
                }
            },
            "hover": {}
        },
        "bundesland": {
            "column": "xnuts2.label",
            "legend": {
                "symbol": {
                    "size": 16,
                    "elements": [{
                        "type": "line",
                        "props": {
                            "x1": 0, "x2": 16, "y1": 8, "y2": 8,
                            "stroke": "@color", "stroke-width": "@stroke-width"
                        }
                    }]
                }
            },
            "props": {
                "common": {
                    "stroke-width": 1.5,
                    "visible": false
                },
                "manual": {
                    "Wien":  { "color": "#1E4F77", "name": "Wien" },
                    "Niederösterreich":  { "color": "#BBD7EE", "name": "Niederösterreich" },
                    "Burgenland":  { "color": "#5BC5F2", "name": "Burgenland"},
                    "Oberösterreich":  { "color": "#559BD5", "name": "Oberösterreich" },
                    "Salzburg": { "color": "#C7DF71", "name": "Salzburg" },
                    "Steiermark": { "color": "#4B9847", "name": "Steiermark" },
                    "Kärnten":  { "color": "#9CE1F8", "name": "Kärnten" },
                    "Tirol":  { "color": "#72BB6F", "name": "Tirol" },
                    "Vorarlberg":  { "color": "#3F766B", "name": "Vorarlberg" },
                    "NA": { "color": "#ABD6A9", "name": "NA"},
                    "Alle": { "color": "#000",  "visible": true, "class": "bold"  }
                }
            },
            "hover": {}
        },
        "var": {
            "column": "variable",
            "props": {
                "common": {
                    "visible": true
                },
                "manual": {
                    "price.m2.n":  { "name": "Beobachtungen" },
                    "price.m2.v":  { "name": "Preis" }
                }
            }
        }
    },
    "facets": {
        "dim": ["var"],
        "cols": 2,
        "scales": []
    },
    "plot": [
        {
            "type": "svg:path",
            "categories": ["quali", "werr", "wrecht", "bundesland", "wfrist"],
            "props": {
                "stroke-width": "2",
                "stroke-dasharray": "@stroke-dasharray",
                "fill": "none",
                "stroke": "@color",
                "d": {
                    "x": "@x:scaled",
                    "y": "@y:scaled"
                }
            }
        },
        {
            "type": "svg:circle",
            "categories": ["quali", "werr", "wrecht", "bundesland", "wfrist"],
            "props": {
                "fill": "@color",
                "r": "3",
                "cx": "@x:scaled",
                "cy": "@y:scaled"
            }
        }
    ]
}
